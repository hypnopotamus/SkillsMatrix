{
  "name": "skills-matrix",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "postinstall": "npm run start:infrastructure",
    "start:infrastructure": "npm run start:build-tools --workspace=localbuildinfrastructure",
    "start:cluster": "npm run start:cluster --workspace=localbuildinfrastructure",
    "prestart": "npm run start:cluster",
    "prebuild": "npm run build --workspace=buildScripts",
    "start": "npm start --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "test": "npm test --workspaces --if-present"
  },
  "workspaces": [
    "infrastructure/local",
    "api",
    "UI",
    "UI/Comparison",
    "buildScripts"
  ]
}
