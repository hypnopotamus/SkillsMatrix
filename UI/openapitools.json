{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.0",
    "generators": {
      "api": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/generated/skills",
        "inputSpec": "http://localhost:3000/swagger-json",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true
        }
      }
    }
  }
}
