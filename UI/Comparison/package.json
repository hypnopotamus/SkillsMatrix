{
  "name": "skills-matrix",
  "version": "0.1.0",
  "main": "dist/server.js",
  "private": true,
  "config": {
    "container": {
      "name": "skills-matrix-ui-comparison",
      "registry": "localhost:5000"
    },
    "chart": {
      "name": "skills-matrix-ui-comparison",
      "repository": "oci://localhost:5000"
    }
  },
  "dependencies": {
    "@skillsmatrix/contentserver": "^1.0.0"
  },
  "scripts": {
    "build": "tsc --project server.tsconfig.json && webpack",
    "build:container": "ts-node scripts/buildContainer.ts",
    "chart:publish": "ts-node scripts/publishChart.ts",
    "prestart": "concurrently \"npm run build:container\" \"npm run chart:publish\"",
    "start": "ts-node scripts/start.ts",
    "serve": "node .",
    "test": "jest",
    "client:generate": "openapi-generator-cli generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.9",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@reduxjs/toolkit": "^1.8.6",
    "@skillsmatrix/api": "^1.0.0",
    "@skillsmatrix/ui-container": "^1.0.0",
    "@types/fabric": "^4.5.12",
    "@types/jest": "^29.1.2",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "fabric": "^4.6.0",
    "fabricjs-react": "^1.0.8",
    "jest": "^29.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
