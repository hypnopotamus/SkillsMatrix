{
  "name": "skills-matrix",
  "version": "0.1.0",
  "main": "dist/server.js",
  "private": true,
  "dependencies": {
    "@skillsmatrix/contentserver": "^1.0.0"
  },
  "scripts": {
    "build": "tsc --project server.tsconfig.json && webpack",
    "prebuild:container": "cd node_modules/localbuildinfrastructure && npm start",
    "build:container": "docker build . --network=host --tag skills-matrix-ui-comparison:latest",
    "postbuild:container": "docker image tag skills-matrix-ui-comparison:latest localhost:5000/skills-matrix-ui-comparison:latest && docker push localhost:5000/skills-matrix-ui-comparison:latest",
    "prestart": "npm run build:container",
    "start": "docker run --detach --publish 127.0.0.1:8082:80 skills-matrix-ui-comparison",
    "serve": "node .",
    "test": "jest",
    "client:generate": "openapi-generator-cli generate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.9",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@reduxjs/toolkit": "^1.8.6",
    "@skillsmatrix/server": "^0.0.1",
    "@skillsmatrix/ui-container": "^1.0.0",
    "@types/fabric": "^4.5.12",
    "@types/jest": "^29.1.2",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "fabric": "^4.6.0",
    "fabricjs-react": "^1.0.8",
    "jest": "^29.2.0",
    "localbuildinfrastructure": "file:../../LocalBuildInfrastructure",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
