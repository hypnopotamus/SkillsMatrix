{
  "version": "1.0.3",
  "name": "@skillsmatrix/ui-container",
  "config": {
    "container": {
      "name": "skills-matrix-ui-container",
      "registry": "localhost:5000",
      "npm": {
        "registry": "http://host.docker.internal:4873/"
      }
    },
    "chart": {
      "name": "skillsmatrix-uicontainer",
      "repository": "oci://localhost:5000"
    }
  },
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --project server.tsconfig.json && webpack",
    "container:build": "npx buildContainer",
    "container:publish": "npx publishContainer",
    "chart:publish": "npx publishChart",
    "prestart": "concurrently \"npm run container:publish\" \"npm run chart:publish\"",
    "start": "npx installChart",
    "serve": "node .",
    "test": "jest",
    "lint": "npx tsc --noEmit",
    "ci:build": "npm run container:build",
    "ci:lint": "concurrently --names \"typescript,helm\" \"npm run lint\" \"helm lint chart\"",
    "ci:publish": "concurrently --names \"container,chart,package\" \"npm run container:publish\" \"npm run chart:publish\" \"npm publish\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hypnopotamus/SkillsMatrix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@microsoft/applicationinsights-clickanalytics-js": "^2.8.9",
    "@microsoft/applicationinsights-react-js": "^3.4.0",
    "@microsoft/applicationinsights-web": "^2.8.9",
    "@mui/material": "^5.10.9",
    "@reduxjs/toolkit": "^1.8.6",
    "@skillsmatrix/buildscripts": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@skillsmatrix/contentserver": "^1.0.1"
  }
}